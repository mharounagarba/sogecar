{
  "name": "exgecar",
  "homepage": "./",
  "version": "0.0.7",
  "description": "Plateforme de gestion pour les courtiers en assurances et r√©assurance.",
  "main": "electron/main.js",
  "author": "3S Consulting",
  "type": "commonjs",
  "license": "MIT",
  "private": false,
  "repository": {
    "type": "git",
    "url": "https://github.com/mharounagarba/sogecar.git"
  },
  "build": {
    "appId": "com.exgecar.apps",
    "productName": "eXgecar",
    "files": [
      "build/",
      "electron/",
      "!**/*.map",
      "!**/*.db",
      "!**/*.md"
    ],
    "directories": {
      "buildResources": "assets"
    },
    "win": {
      "target": [
        "nsis"
      ],
      "icon": "assets/icon.ico",
      "cscLink": "private/eXgecar.pfx",
      "verifyUpdateCodeSignature": false,
      "legalTrademarks": "eXgecar, produit 3S Consulting"
    },
    "mac": {
      "target": [
        "dmg"
      ],
      "icon": "assets/icon.icns"
    },
    "linux": {
      "target": [
        "AppImage"
      ],
      "icon": "assets/icon.png"
    },
    "publish": {
      "provider": "github",
      "owner": "mharounagarba",
      "repo": "sogecar",
      "private": false,
      "releaseType": "release"
    },
    "nsis": {
      "oneClick": false,
      "allowToChangeInstallationDirectory": true,
      "installerIcon": "assets/icon.ico",
      "uninstallerIcon": "assets/icon.ico",
      "installerHeaderIcon": "assets/icon.ico",
      "createDesktopShortcut": true,
      "createStartMenuShortcut": true
    }
  },
  "electronDownload": {
    "useSymLinks": false
  },
  "scripts": {
    "dev": "concurrently \"vite\" \"wait-on http://localhost:5173 && electron .\"",
    "build": "vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "electron": "electron electron/main.js",
    "electron-dev": "concurrently \"yarn dev\" \"wait-on http://localhost:5173 && electron .\"",
    "rebuild": "electron-rebuild -f -w better-sqlite3",
    "dist": "electron-builder -w",
    "deploy": "electron-builder build -w -p always",
    "postinstall": "electron-builder install-app-deps",
    "make-cert": "electron-builder create-self-signed-cert -p eXgecar"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^2.8.2",
    "aos": "^2.3.4",
    "bcryptjs": "^3.0.2",
    "better-sqlite3": "^11.10.0",
    "dayjs": "^1.11.13",
    "electron-updater": "^6.6.2",
    "exceljs": "^4.4.0",
    "file-saver": "^2.0.5",
    "html2canvas": "^1.4.1",
    "html2pdf.js": "^0.10.3",
    "jspdf": "^3.0.1",
    "jspdf-autotable": "^5.0.2",
    "pdf-lib": "^1.17.1",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-icons": "^5.5.0",
    "react-redux": "^9.2.0",
    "react-router-dom": "^7.6.1",
    "recharts": "^2.15.3",
    "uuid": "^11.1.0",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@eslint/js": "^9.25.0",
    "@types/react": "^19.1.2",
    "@types/react-dom": "^19.1.2",
    "@vitejs/plugin-react": "^4.4.1",
    "concurrently": "^9.1.2",
    "electron": "^36.3.2",
    "electron-builder": "^26.0.12",
    "electron-rebuild": "^3.2.9",
    "eslint": "^9.25.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "globals": "^16.0.0",
    "vite": "^6.3.5",
    "wait-on": "^8.0.3"
  },
  "packageManager": "yarn@1.22.22+sha512.a6b2f7906b721bba3d67d4aff083df04dad64c399707841b7acf00f6b133b7ac24255f2652fa22ae3534329dc6180534e98d17432037ff6fd140556e2bb3137e"
}
